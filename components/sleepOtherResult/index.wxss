.containers {
  height: 100%;
  box-sizing: border-box;
  padding: 10rpx 30rpx;
}
.result_content {
  background-color: #fff;
  border-radius: 12rpx;
  padding: 36rpx 24rpx;
  padding-bottom: 24rpx;
  box-shadow: 0rpx 4rpx 13rpx 0rpx #E8EDEC;
}
.title text {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 700;
  color: rgba(0, 0, 0, 1);
  line-height: 45rpx;
}

.test-result {
  margin-top: 34rpx;
}

.test-result image {
  width: 204rpx;
  height: 253rpx;
}

.r-1 {
  font-size: 25rpx;
  font-family: PingFangSC-Regular;
  font-weight: 700;
  color: #4D4D4D;
  line-height: 40rpx;
  display: flex;
  flex-flow: row;
  width: 100%;
}

.r-t {
  color: #00BFFF;
  padding: 0;
}

.pre-next-content {
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  margin-top: 25rpx;
}

.pre-next-btn {
  width: 100%;
  background-color: #36BC96;
  border-radius: 12rpx;
  height: 74rpx;
  line-height: 74rpx;
  font-size: 30rpx;
  text-align: center;
  color: #fff;
}
.left_btn {
  margin-right: 37rpx;
  color: #36BC96;
  background-color: #E7E7E7;
}
/* 文章 */
.show_article {
  margin-top: 24rpx;
  background-color: #fff;
  border-radius: 12rpx;
  box-shadow: 0rpx 4rpx 13rpx 0rpx #E8EDEC;
  padding: 36rpx 24rpx 26rpx;
}